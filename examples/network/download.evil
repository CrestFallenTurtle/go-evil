@ Downloads a file from the provied url  @

use system
use network

[
    version 2.0
    output download
    os linux
    arch amd64
    obfuscate false
    debugger_behavior stop
]


l main_func {
    network::download("http://ipv4.download.thinkbroadband.com/512MB.zip") @ Downloads the provided file @

    system::exit("0")
}