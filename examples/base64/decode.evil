@ Decodes a base64 string @

use system
use base64

[
    version 2.0
    output decode
    os linux
    arch amd64
    obfuscate false
    debugger_behavior stop
]


l main_func {
    base64::decode("dGhlIGpvdXJuZXkgc3VyZSB3YXMgZnVu") @ The result is placed in a runtime variable @
    system::outln("The result was, €1€")
    system::exit("0")
}