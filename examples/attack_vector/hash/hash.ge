@ Example showcasing a basic MD5 hash

version 1.0;

main:{
    attack.set_hash("md5");
    attack.hash("hello world!");
    system.out("MD5 gave $1 instead of 'hello world!'"); @ Prints "fc3ff98e8c6a0d3087d515c0473f8677"
    
    system.out("$2"); @ Prints NULL
    attack.hash("hello again!");
    system.out("MD5 gave $2 instead of 'hello again!'"); @ Now prints "cf890860d911e0ece33e648fc5fcd1e0" 

    system.exit("1");
}