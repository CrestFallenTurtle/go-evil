@ Creates a file on the disk, and copies it @

use system

[
    version 2.0
    output copy
    os linux
    arch amd64
    obfuscate false
    debugger_behavior stop
]


l main_func {
    system::write(${"./test.txt", "hello, world!\n"}$)
    system::copy(${"./test.txt", "./test_copy.txt"}$)
    system::exit("0")
}