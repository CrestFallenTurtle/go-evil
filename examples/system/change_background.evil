@ Changes the background image on your computer @

@ You must run this example from the root directory of the project for it to work! @

use system

[
    version 2.0
    output change_background
    os linux
    arch amd64
    obfuscate false
    debugger_behavior stop
]

l main_func {
    system::change_background("€39€/examples/system/frog.jpg")
    system::exit("0")
}