@ Creates a new user on the local machine @

use system

[
    version 2.0
    output create_user
    os linux
    arch amd64
    obfuscate false
    debugger_behavior stop
]

b elevation {
    system::elevate()  @ Hopefully we get administrator @
}

l main_func {
    system::create_user(${"test", "test"}$) @ Username and password @
    system::exit("0")
}