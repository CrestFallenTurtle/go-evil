@ Showcasing how you can lock the keyboard, enter a website and then unlock the keyboard
[version 1.1]
main:{
    keyboard.lock();
    window.set.x("600"); @ Widht on the x-axis
	window.set.y("800"); @ Height on the y-axis
	window.set.title("You just got hacked bruh!");
	window.goto("https://mrskeltal.com/");
    keyboard.unlock();
    system.exit("0");
}